class Person:

    def __init__(self, name age, email):
        self.name = name
        self.age = age
        self.email = email



bob = Person("Bob", 29, "bob@flatironschool.com")
avi = Person("Avi", 28, "avi@flatironschool.com")
adam = Person("Adam", 28, "adam@flatironschool.com")

# A table in SQL to a class in Python
# A row in SQL to an instance/object in Python